.decl E(a:number,b:number)
.input E
.decl P(a:number,b:number)
.output P
.decl E_0(a:number,b:number)
E_0(a,b) :- E(a,b), a % 8 = 0.
.decl P_0(a:number,b:number)
P_0(a,b) :- E_0(a,b). P_0(a,b) :- P_0(a,x), P_0(x,b). 
.decl E_1(a:number,b:number)
E_1(a,b) :- E(a,b), a % 8 = 1.
.decl P_1(a:number,b:number)
P_1(a,b) :- E_1(a,b). P_1(a,b) :- P_1(a,x), P_1(x,b). 
