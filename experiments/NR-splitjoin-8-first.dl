P_5(a,b) :- E_5(a,b). P_5(a,b) :- P_5(a,x), P_5(x,b). 
.decl E_6(a:number,b:number)
E_6(a,b) :- E(a,b), a % 8 = 6.
.decl P_6(a:number,b:number)
P_6(a,b) :- E_6(a,b). P_6(a,b) :- P_6(a,x), P_6(x,b). 
.decl E_7(a:number,b:number)
E_7(a,b) :- E(a,b), a % 8 = 7.
.decl P_7(a:number,b:number)
P_7(a,b) :- E_7(a,b). P_7(a,b) :- P_7(a,x), P_7(x,b). 
P(a,b) :- P_0(a,b).
P(a,b) :- P_1(a,b).
P(a,b) :- P_2(a,b).
P(a,b) :- P_3(a,b).
P(a,b) :- P_4(a,b).
P(a,b) :- P_5(a,b).
P(a,b) :- P_6(a,b).
P(a,b) :- P_7(a,b).
P(a,b) :- P(a,x), P(x,b).
